{
    "presets": ["babel-preset-expo"],
    "env": {
        "development": {
            "plugins": ["transform-react-jsx-source"]
        }
    },
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "_navigation": "./src/_navigation",
                    "_redux": "./src/_redux",
                    "_page": "./src/_page",
                    "_database": "./src/_database"
                }
            }
        ]
    ]
}
